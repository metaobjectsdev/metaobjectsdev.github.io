    <!-- Quick Start Section -->
    <section class="quickstart">
        <div class="container">
            <div class="section-header">
                <h2>Get Started in Minutes</h2>
                <p>From zero to running MetaObjects application in three simple steps</p>
            </div>
            <div class="quickstart-steps">
                <div class="step">
                    <div class="step-number">1</div>
                    <h3>Add Dependency</h3>
                    <div class="code-block">
                        <pre><code class="language-xml">&lt;dependency&gt;
  &lt;groupId&gt;com.draagon.metaobjects&lt;/groupId&gt;
  &lt;artifactId&gt;metaobjects-core&lt;/artifactId&gt;
  &lt;version&gt;5.1.0&lt;/version&gt;
&lt;/dependency&gt;</code></pre>
                    </div>
                </div>
                <div class="step">
                    <div class="step-number">2</div>
                    <h3>Define Metadata</h3>
                    <div class="code-block">
                        <pre><code class="language-json">{
  "object": {
    "name": "Product",
    "type": "pojo",
    "children": [
      {"field": {"name": "id", "type": "long"}},
      {"field": {"name": "name", "type": "string"}},
      {"field": {"name": "price", "type": "double"}}
    ]
  }
}</code></pre>
                    </div>
                </div>
                <div class="step">
                    <div class="step-number">3</div>
                    <h3>Generate & Use</h3>
                    <div class="code-block">
                        <pre><code class="language-bash">mvn metaobjects:generate</code></pre>
                    </div>
                    <div class="code-block">
                        <pre><code class="language-java">MetaObject productMeta = loader.getMetaObject("Product");
Object product = productMeta.newInstance();
productMeta.setFieldValue(product, "name", "Widget");
productMeta.setFieldValue(product, "price", 29.99);</code></pre>
                    </div>
                </div>
            </div>
        </div>
    </section>

